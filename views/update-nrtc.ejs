            <!-- Training Date -->
            <div class="card mb-3">
                <div class="card-header">
                    Update Training Date</div>
                <div class="card-body">
                    <form action="/trainingDate" method="POST">

                        <div class="form-group">
                            <div class="form-label-group" >
                                <input type="text" name="dateText" id="dateText" class="form-control w-100" placeholder="Enter new date here eg.12th March, 2019" required="required" autofocus="autofocus">
                                <label for="dateText">Enter new date here eg.12th March, 2019</label>
                            </div>
                        </div>

                        <input type="submit" class="btn btn-primary btn-block">
                    </form>

                </div>
                <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
            </div>

            <div style="height: 50px;"></div>

            <!-- News and Opportunities -->
            <div class="card mb-3">
                <div class="card-header">
                    News and Opportunities Page</div>
                <div class="card-body">
                    <form action="/updateBlog" method="POST" enctype="multipart/form-data" >
                        <div class="form-group">
                            <div class="form-label-group">
                                <input type="text" name="blogtitle" id="blogtitle" class="form-control" placeholder="Enter Title here" required="required" autofocus="autofocus">
                                <label for="blogtitle">Enter Title here</label>
                            </div>
                        </div>
                        <div class="form-group">
                            <textarea type="text" name="blogmessage" rows="15" cols="100%" required="required"></textarea>
                        </div>
                        <div class="form-group">
                            <div class="form-label-group">
                                 <input type="file" name="file" class="form-control" accept="image/*" placeholder="Upload Blog Image"/>
                                <label for="file">Upload Blog Image</label>
                            </div>
                        </div>
                        <input type="submit" class="btn btn-primary btn-block">
                    </form>

                </div>
                <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
            </div>

            <div style="height: 50px;"></div>

            
            <!-- Update Student and Funded Research -->
            <div class="card mb-3">
                <div class="card-header">
                    <div class="w3-bar  w3-border">
                         <a class="w3-bar-item w3-button  w3-border  w3-hover-text-brown" style="cursor: pointer" onclick="openCity('London')" href="#London">Funded-Research</a> 
                         <a class="w3-bar-item w3-button w3-border w3-hover-text-brown" style="cursor: pointer" onclick="openCity('Tokyo')" href="#Tokyo">Student-Research</a>    
                    </div>
                            
                            <!-- Student research here -->
                            <div class="card-body w3-container city" id="Tokyo">
                                <h3 class="w3-center w3-border-bottom">Student Research Page</h3>
                                    <form action="/StudentResearch" method="POST">
                                        <div class="form-group">
                                            <div class="form-label-group w3-margin-top">
                                                <input type="text" name="title" id="inputText1" class="form-control" placeholder="Enter Title here" required="required" autofocus="autofocus">
                                                <label for="inputText1">Enter Title here</label>
                                            </div> 
                                            <div class="form-label-group w3-margin-top">
                                                    <input type="text" name="studentName" id="student" class="form-control" placeholder="Student FullName" required="required" autofocus="autofocus">
                                                    <label for="student">Student FullName</label>
                                                </div>

                                                <div class="form-label-group w3-margin-top">
                                                        <input type="text" name="degree" id="degree" class="form-control" placeholder="eg. Doctorate, University of Ghana, Legon" required="required" autofocus="autofocus">
                                                        <label for="degree">Degree and Institution</label>
                                                    </div>

                                        </div>
                                        <div class="form-group">
                                            <textarea type="text" name="message" rows="15" cols="100%" required="required"></textarea>
                                        </div>
                                     <input type="submit" class="btn btn-primary btn-block">
                                    </form>
                
                                </div>

                            <!-- Funded research here -->
                <div class="card-body w3-container city" id="London">
                    
                        <h3 class="w3-center w3-border-bottom">Funded Research Page</h3>
                    <div>
                    <form action="/FundedResearch" method="POST" enctype="multipart/form-data" >
                        <div class="form-group w3-margin-top">
                            <div class="form-label-group">
                                <input type="text" name="fundedTitle" id="fundedTitle" class="form-control" placeholder="Enter Title here" required="required" autofocus="autofocus">
                                <label for="fundedTitle">Enter Title here</label>
                            </div>
                        </div>
                        <div class="form-group w3-margin-top">
                                <div class="form-label-group">
                                    <input type="text" name="researcher" id="researcher" class="form-control" placeholder="Researcher's FullName" required="required" autofocus="autofocus">
                                    <label for="researcher">Researcher's FullName</label>
                                </div>
                            </div>
                        <div class="form-group">
                            <textarea type="text" name="message" rows="15" cols="100%" required="required"></textarea>
                        </div>
                        <div class="form-group">
                            <div class="form-label-group">
                                <input type="file" name="file" class="form-control" accept="image/*" placeholder="Upload Image"/>
                                <label for="file">Upload Image</label>
                            </div>
                        </div>
                        <input type="submit" class="btn btn-primary btn-block">
                    </form> 
               </div>
               
